<Window x:Class="BrowserHistory_Server.View.Window_Settings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BrowserHistory_Server.View"
        mc:Ignorable="d"
        Title="Window_Settings" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="MainDataGrid" Grid.Row="1" Margin="0,44.6,-0.4,0.4" Grid.RowSpan="3" Background="White">
            <DataGrid.Columns>
                <DataGridTextColumn Binding="{x:Null}" ClipboardContentBinding="{x:Null}" Header="IP"/>
                <DataGridTextColumn Binding="{x:Null}" ClipboardContentBinding="{x:Null}" Header="ФИО"/>
                <DataGridTextColumn Binding="{x:Null}" ClipboardContentBinding="{x:Null}" Header="Геолокация"/>
                <DataGridTextColumn Binding="{x:Null}" ClipboardContentBinding="{x:Null}"  Header="История"/>
            </DataGrid.Columns>

        </DataGrid>
    </Grid>
</Window>
